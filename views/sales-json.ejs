<div class="container">
<table class= "table">
    <thead>
    <tr>
    <th scope = "col" > ID </th>
    <th scope = "col" > name </th>
    <th scope = "col" > age </th>
    </thead>

    <tbody>
    <% for(let i of sales){ %>
    <tr>
